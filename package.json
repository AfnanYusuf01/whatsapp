{
  "name": "whatsapp",
  "version": "1.0.0",
  "description": "WhatsApp application with Flowbite Admin Dashboard",
  "type": "module",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "run-p start:dev:*",
    "start:dev:server": "nodemon src/app.js",
    "start:dev:webpack": "webpack --mode=development --watch",
    "build": "run-s build:webpack",
    "build:webpack": "webpack --mode=production",
    "build:css": "tailwindcss build -i src/style.css -o static/app.css",
    "seed": "node database/seeders/userSeeder.js",
    "seed:force": "node database/seeders/forceSeed.js"
  },
  "keywords": [
    "whatsapp",
    "admin dashboard",
    "tailwind",
    "flowbite",
    "express",
    "ejs"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.18.3",
    "express-ejs-layouts": "^2.5.1",
    "express-fileupload": "^1.5.1",
    "form-data": "^4.0.3",
    "jsonwebtoken": "^9.0.2",
    "midtrans-client": "^1.4.3",
    "ngrok": "^5.0.0-beta.2"
  },
  "devDependencies": {
    "apexcharts": "^3.41.0",
    "autoprefixer": "^10.4.14",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "flowbite": "^2.1.1",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "postcss-loader": "^7.3.3",
    "prisma": "^5.10.2",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.2",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  }
}
