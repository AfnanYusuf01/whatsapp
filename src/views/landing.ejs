    <!-- Pricing Section -->
    <section id="pricing" class="pricing section">
      <div class="container section-title" data-aos="fade-up">
        <h2>Pricing</h2>
        <p>Necessitatibus eius consequatur ex aliquid fuga eum quidem sint consectetur velit</p>
      </div>
      <div class="container">
        <div class="row gy-4">
          <% if (subscriptions && subscriptions.length > 0) { %>
            <% subscriptions.forEach(function(subscription) { %>
              <div class="col-lg-4" data-aos="zoom-in">
                <div class="pricing-item <%= subscription.featured ? 'featured' : '' %>">
                  <% if (subscription.featured) { %><p class="popular">Popular</p><% } %>
                  <h3><%= subscription.name %></h3>
                  <p class="description"><%= subscription.description || 'Perfect for your business needs.' %></p>
                  <h4>
                    <% if (parseFloat(subscription.price) === 0) { %>
                      Free
                    <% } else { %>
                      <sup>Rp</sup><%= new Intl.NumberFormat('id-ID').format(parseFloat(subscription.price)) %><span> / month</span>
                    <% } %>
                  </h4>
                  <ul>
                    <li><i class="bi bi-check"></i> <span><%= subscription.limit_device > 999 ? 'Unlimited' : subscription.limit_device %> WhatsApp Device<%= subscription.limit_device > 1 ? 's' : '' %></span></li>
                    <li><i class="bi bi-check"></i> <span>Access for <%= subscription.duration_days %> days</span></li>
                    <li><i class="bi bi-check"></i> <span>Dashboard Analytics</span></li>
                    <li><i class="bi bi-check"></i> <span>Smart Message Management</span></li>
                    <% if (parseFloat(subscription.price) > 0) { %>
                      <li><i class="bi bi-check"></i> <span>Advanced CRM Features</span></li>
                      <li><i class="bi bi-check"></i> <span>Anti-Ban Protection</span></li>
                      <% if (parseFloat(subscription.price) > 500000) { %>
                        <li><i class="bi bi-check"></i> <span>Premium support: <span class="fw-bold">24/7</span></span></li>
                        <li><i class="bi bi-check"></i> <span>Custom Integration</span></li>
                      <% } else { %>
                        <li><i class="bi bi-check"></i> <span>Premium support: <span class="fw-bold">Email & Chat</span></span></li>
                      <% } %>
                    <% } else { %>
                      <li><i class="bi bi-check"></i> <span>Basic Support</span></li>
                    <% } %>
                  </ul>
                  <% if (typeof user !== 'undefined' && user) { %>
                    <form action="/order" method="POST">
                      <input type="hidden" name="subscription_id" value="<%= subscription.id %>" />
                      <button type="submit" class="cta-btn">Pesan Sekarang</button>
                    </form>
                  <% } else { %>
                    <a href="/register" class="cta-btn">
                      <% if (parseFloat(subscription.price) === 0) { %>
                        Get started
                      <% } else { %>
                        Choose <%= subscription.name %>
                      <% } %>
                    </a>
                  <% } %>
                </div>
              </div>
            <% }); %>
          <% } else { %>
            <!-- Fallback static pricing card -->
            <div class="col-lg-4" data-aos="zoom-in">
              <div class="pricing-item">
                <h3>Starter</h3>
                <p class="description">Best option for personal use & for your next project.</p>
                <h4>Free</h4>
                <ul>
                  <li><i class="bi bi-check"></i> <span>1 WhatsApp Device</span></li>
                  <li><i class="bi bi-check"></i> <span>Basic Support</span></li>
                </ul>
                <a href="/register" class="cta-btn">Get started</a>
              </div>
            </div>
          <% } %>
        </div>
      </div>
    </section>

