                                    <div class="flex justify-between">
                                        <span class="text-sm font-medium text-gray-500 dark:text-gray-400">Favicon URL:</span>
                                        <span class="text-sm text-gray-900 dark:text-white">${agency.favicon_url || '-'}</span>
